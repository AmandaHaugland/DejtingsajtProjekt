@using DejtingsajtProjekt.Models

@model ProfileListViewModel


<h2>Some of our users</h2>
<div class="list-group">

    @foreach (var profile in Model.Profiles)
    { 
    <a href="/Profile/UserProfile/@profile.UserId" class="list-group-item col-sm-3">
        @profile.Firstname  @profile.Lastname
        <p>
            @if (profile.ImageName != null)
            {
                <img src="@Url.Content(profile.ImageName)" style="height:30px;width:30px;" />
            }
        </p>
    </a> 
    }

</div>
