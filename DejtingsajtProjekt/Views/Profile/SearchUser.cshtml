@model IEnumerable<DejtingsajtProjekt.Models.ProfileModel>
@{
    ViewBag.Title = "SearchUser";
}
<h2>SearchUser</h2>
<p>
    @using (Html.BeginForm())
    {
<p>
    Search firstname: @Html.TextBox("firstname") <br />
    Search Lastame:@Html.TextBox("lastname") <br />
    <input type="submit" name="submit" value="Search" />
</p>
}
    </p>

   <table>
    @foreach (var profile in Model)
    {

       <tr>
           <td><a class="list-group-item" href="/Profile/UserProfile/@profile.UserId">
    @profile.Firstname  @profile.Lastname
                   <p>
                       @if (profile.ImageName != null)
                       {
                           <img src="@Url.Content(profile.ImageName)"  style="height:50px;width:50px;"/>
                       }
                   </p>
    </a>
           </td> 
       </tr>
    }
    </table>

 


